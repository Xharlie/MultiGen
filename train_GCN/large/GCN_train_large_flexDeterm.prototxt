# deconv: output_size = (input_size - 1) * stride + kernel_size - 2 * pad
name: "DeconvMultiGenFaceNet"
layer {
  name: "data"
  type: "HDF5Data"
  top: "data"
  top: "person"
  top: "personclass"
  top: "emotion"
  top: "emotionclass"
  top: "session"
  top: "transform"
  include {
    phase: TRAIN
  }
  hdf5_data_param {
    source: "../../Source/GCN/all_info_large_session0_4emo.txt"
    batch_size: 32
    shuffle : true
  }
}

layer {
  name: "fc1_person"
  type: "InnerProduct"
  bottom: "person"
  top: "fc1_person"
  include {
    phase: TRAIN
  }
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 1
    decay_mult: 1
  }
  inner_product_param {
    num_output: 512
    weight_filler {
      type: "xavier"
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}

layer {
  name: "relu1_person"
  type: "ReLU"
  bottom: "fc1_person"
  top: "fc1_person"
  include {
    phase: TRAIN
  }
   relu_param {
     negative_slope: 0.1
   }
}
layer {
  name: "fc2_person"
  type: "InnerProduct"
  bottom: "fc1_person"
  top: "fc2_person"
  include {
    phase: TRAIN
  }
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 1
    decay_mult: 1
  }
  inner_product_param {
    num_output: 512
    weight_filler {
      type: "xavier"
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}
layer {
  name: "relu2_person"
  type: "ReLU"
  bottom: "fc2_person"
  top: "fc2_person"
  include {
    phase: TRAIN
  }
  relu_param {
     negative_slope: 0.1
  }
}

layer {
  name: "fc1_emotion"
  type: "InnerProduct"
  bottom: "emotion"
  top: "fc1_emotion"
  include {
    phase: TRAIN
  }
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 1
    decay_mult: 1
  }
  inner_product_param {
    num_output: 512
    weight_filler {
      type: "xavier"
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}
layer {
  name: "relu1_emotion"
  type: "ReLU"
  bottom: "fc1_emotion"
  top: "fc1_emotion"
  include {
    phase: TRAIN
  }
    relu_param {
     negative_slope: 0.1
    }
}
layer {
  name: "fc2_emotion"
  type: "InnerProduct"
  bottom: "fc1_emotion"
  top: "fc2_emotion"
  include {
    phase: TRAIN
  }
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 1
    decay_mult: 1
  }
  inner_product_param {
    num_output: 512
    weight_filler {
      type: "xavier"
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}
layer {
  name: "relu2_emotion"
  type: "ReLU"
  bottom: "fc2_emotion"
  top: "fc2_emotion"
  include {
    phase: TRAIN
  }
   relu_param {
     negative_slope: 0.1
   }
}
#
layer {
  name: "fc1_session"
  type: "InnerProduct"
  bottom: "session"
  top: "fc1_session"
  include {
    phase: TRAIN
  }
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 1
    decay_mult: 1
  }
  inner_product_param {
    num_output: 128
    weight_filler {
      type: "xavier"
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}
layer {
  name: "relu1_session"
  type: "ReLU"
  bottom: "fc1_session"
  top: "fc1_session"
  include {
    phase: TRAIN
  }
   relu_param {
     negative_slope: 0.1
   }
}

layer {
  name: "fc2_session"
  type: "InnerProduct"
  bottom: "fc1_session"
  top: "fc2_session"
  include {
    phase: TRAIN
  }
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 1
    decay_mult: 1
  }
  inner_product_param {
    num_output: 128
    weight_filler {
      type: "xavier"
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}
layer {
  name: "relu2_session"
  type: "ReLU"
  bottom: "fc2_session"
  top: "fc2_session"
  include {
    phase: TRAIN
  }
   relu_param {
     negative_slope: 0.1
   }
}

layer {
  name: "fc1_transform"
  type: "InnerProduct"
  bottom: "transform"
  top: "fc1_transform"
  include {
    phase: TRAIN
  }
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 1
    decay_mult: 1
  }
  inner_product_param {
    num_output: 256
    weight_filler {
      type: "xavier"
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}
layer {
  name: "relu1_transform"
  type: "ReLU"
  bottom: "fc1_transform"
  top: "fc1_transform"
  include {
    phase: TRAIN
  }
   relu_param {
     negative_slope: 0.1
   }
}
layer {
  name: "fc2_transform"
  type: "InnerProduct"
  bottom: "fc1_transform"
  top: "fc2_transform"
  include {
    phase: TRAIN
  }
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 1
    decay_mult: 1
  }
  inner_product_param {
    num_output: 256
    weight_filler {
      type: "xavier"
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}
layer {
  name: "relu2_transform"
  type: "ReLU"
  bottom: "fc2_transform"
  top: "fc2_transform"
  include {
    phase: TRAIN
  }
   relu_param {
     negative_slope: 0.1
   }
}

layer {
  name: "concat"
  type: "Concat"
  bottom: "fc2_person"
  bottom: "fc2_emotion"
  bottom: "fc2_session"
  bottom: "fc2_transform"
  top: "fc2_gen"
  include {
    phase: TRAIN
  }
  concat_param {
    concat_dim: 1
  }
}

layer {
  name: "fc3_gen"
  type: "InnerProduct"
  bottom: "fc2_gen"
  top: "fc3_gen"
  include {
    phase: TRAIN
  }
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 1
    decay_mult: 1
  }
  inner_product_param {
    num_output: 2048
    weight_filler {
      type: "xavier"
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}

layer {
  name: "relu3_gen"
  type: "ReLU"
  bottom: "fc3_gen"
  top: "fc3_gen"
  include {
    phase: TRAIN
  }
   relu_param {
     negative_slope: 0.1
   }
}
layer {
  name: "fc4_gen"
  type: "InnerProduct"
  bottom: "fc3_gen"
  top: "fc4_gen"
  include {
    phase: TRAIN
  }
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 1
    decay_mult: 1
  }
  inner_product_param {
    num_output: 2048
    weight_filler {
      type: "xavier"
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}
layer {
  name: "relu4_gen"
  type: "ReLU"
  bottom: "fc4_gen"
  top: "fc4_gen"
  include {
    phase: TRAIN
  }
   relu_param {
     negative_slope: 0.1
   }
}

layer {
  name: "fc5_gen"
  type: "InnerProduct"
  bottom: "fc4_gen"
  top: "fc5_gen"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 1
    decay_mult: 1
  }
  include {
    phase: TRAIN
  }
  inner_product_param {
    num_output: 32768
    weight_filler {
      type: "xavier"
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}
layer {
  name: "relu5_gen"
  type: "ReLU"
  bottom: "fc5_gen"
  top: "fc5_gen"
  include {
    phase: TRAIN
  }
   relu_param {
     negative_slope: 0.1
   }
}
layer {
  name: "reshape"
  type: "Reshape"
  bottom: "fc5_gen"
  top: "fc5_reshape"
  include {
    phase: TRAIN
  }
  reshape_param {
     shape {
        dim: -1
        dim: 512
        dim: 8
        dim: 8
     }
  }
}

layer {
  name: "deconv6"
  type: "Deconvolution"
  bottom: "fc5_reshape"
  top: "deconv6"
  include {
    phase: TRAIN
  }
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 1
    decay_mult: 1
  }
  convolution_param {
    num_output: 512
    pad: 0
    kernel_size: 5
    stride: 2
    weight_filler {
      type: "xavier"
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}

layer {
  name: "relu6_gen"
  type: "ReLU"
  bottom: "deconv6"
  top: "deconv6"
  include {
    phase: TRAIN
  }
  relu_param {
     negative_slope: 0.1
  }
}
